{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended-with-formatting",
		"plugin:prettier/recommended",
		"prettier/react"
	],
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module"
	},
	"globals": {
		"browser": true
	},
	"settings": {
		"jsdoc": {
			"mode": "typescript"
		}
	},
	"rules": {
		"prettier/prettier": "error"
	}
}
